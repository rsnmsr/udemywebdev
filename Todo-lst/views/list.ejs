<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>To do list</title>
    <link rel="stylesheet" href="css/styles.css">
</head>

<body>
    <div class="box" id="heading">
        <h1><%=ListTitle%> </h1>
    </div>


    <div class="box">

        <%   for(var i=0;i<new_list_item.length;i++){  %>

        <div class="item">
            <input type="checkbox">
          

            <p><%=new_list_item[i]%> </p>

           
        </div>
        <%  } %>

        <form action="/" method="post" class="item">
            <input type="text" name="todo_item" placeholder="Work to do" autocomplete="off">
            <button type="submit" name="list" value="<%=ListTitle%>">+</button>
    
        </form>


    </div>

</body>

</html>